package com.xworkz.linkedlist;

import java.util.LinkedList;

public class Runner {
	public static void main(String[] args) {
		
		LinkedList<String> list = new LinkedList<String>();
		
		list.add("peacock");
		list.add("parrot");
		list.add("pigeon");
		list.add("bulbull");
		list.add("Eagle");
		list.add("Kingfisher");
		System.out.println(list.toString());
		
		list.add(0, "peck");
		System.out.println(list.toString());
		
		
//		//peekFirst
